{
    "name": "Image Editor",
    "version": "1.0.0",
    "description": "Increase contrast or remove background from images.",
    "manifest_version": 3,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js": ["js/jquery.js","js/contentScript.js"]
    }],
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "permissions": [
        "https://some-random-api.ml/*",
        "scripting",
        "activeTab"
    ],
    "icons": {
      "48": "images/icon48.png"
    },
    "action": {
        "default_title": "Image Editor",
        "default_popup": "index.html"
    }
}
  