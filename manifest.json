{
  "manifest_version": 3,

  "name": "Silver Sonics",
  "description": "High Contrast Chrome Extenstion",
  "version": "1.0.0",
  "default_icon": {
    "48": "icon48.png"
  },
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Silver Sonics"
  }
}
